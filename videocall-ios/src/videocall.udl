namespace videocall {
    string hello_world();
    string get_version();
}; 

[Error]
enum WebTransportError {
    "ConnectionFailed",
    "InvalidUrl",
    "StreamError",
    "HttpError",
    "TlsError",
    "Unknown",
};

interface WebTransportClient {
    constructor(string url);
    [Throws=WebTransportError]
    void connect();
    [Throws=WebTransportError]
    sequence<u8> send_datagram(sequence<u8> data);
};